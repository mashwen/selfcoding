<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://dubbo.apache.org/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://dubbo.apache.org/schema/dubbo http://dubbo.apache.org/schema/dubbo/dubbo.xsd">
    <!--1、命名-->
    <dubbo:application name="lxuserconsumer"></dubbo:application>
    <!--2、注册中心-->
    <dubbo:registry protocol="zookeeper" address="39.105.189.141:2181"></dubbo:registry>
    <!--3、传输协议-->
    <dubbo:protocol name="dubbo" port="20880"></dubbo:protocol>
    <!--4、发布服务-->
    <dubbo:reference interface="com.feri.service.user.UserService" check="false" retries="1" id="userService"></dubbo:reference>
</beans>